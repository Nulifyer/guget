{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug - Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/guget",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "postDebugTask": "reset-test-dotnet",
            "cwd": "${workspaceFolder}",
            "env": {
                "LOG_LEVEL": "trace"
            },
            "args": [
                "-v", "dbg",
                "-p", "./test-dotnet",
                "-lf", "./guget.log.txt",
                "-t", "gruvbox",
                "--", "--extra-arg1", "value1", "--extra-arg2", "value2", "-p", "./fake-project"
            ]
        },
        {
            "name": "Debug - Normal",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/guget/main.go",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "postDebugTask": "reset-test-dotnet",
            "cwd": "${workspaceFolder}",
            "args": [
                "-v", "trc",
                "-p", "./test-dotnet",
            ]
        },
        {
            "name": "Run VSCode Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/vscode-guget",
                "${workspaceFolder}/test-dotnet"
            ]
        }
    ]
}
