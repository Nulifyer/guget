{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "create-debug-dotnet",
            "postDebugTask": "remove-debug-dotnet",
            "args": [
                "-p ./debug-dotnet"
            ]
        }
    ]
}