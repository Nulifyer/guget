{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug - Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/guget",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "create-debug-dotnet",
            "postDebugTask": "remove-debug-dotnet",
            "cwd": "${workspaceFolder}",
            "env": {
                "LOG_LEVEL": "trace"
            },
            "args": [
                "-v", "dbg",
                "-p", "./debug-dotnet",
                "--", "--extra-arg1", "value1", "--extra-arg2", "value2", "-p", "./fake-project"
            ]
        },
        {
            "name": "Debug - Normal",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/guget/main.go",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "create-debug-dotnet",
            "postDebugTask": "remove-debug-dotnet",
            "cwd": "${workspaceFolder}",
            "args": [
                "-v", "trc",
                "-p", "./debug-dotnet",
            ]
        }
    ]
}