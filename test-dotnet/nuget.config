<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="dotnet9" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet9/nuget/v3/index.json" />
    <add key="dotnet-public" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-public/nuget/v3/index.json" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="github-nulifyer" value="https://nuget.pkg.github.com/Nulifyer/index.json" />
    <add key="github-packages-examples" value="https://nuget.pkg.github.com/github-packages-examples/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <github-nulifyer>
      <add key="Username" value="Nulifyer" />
      <add key="ClearTextPassword" value="REPLACE_WITH_PAT" />
    </github-nulifyer>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="dotnet-public">
      <package pattern="Microsoft.*" />
      <package pattern="System.*" />
    </packageSource>
    <packageSource key="dotnet9">
      <package pattern="Microsoft.Extensions.*" />
    </packageSource>
    <packageSource key="github-nulifyer">
      <package pattern="Guget.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
